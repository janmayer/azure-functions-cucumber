{
    "request": {
        "urlPathPattern": "/.*/oauth2/v2.0/token",
        "method": "POST"
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "token_type": "bearer",
            "expires_in": 5,
            "ext_expires_in": 5,
            "access_token": "im.a.token"
        },
        "headers": {
            "Content-Type": "application/json"
        }
    }
}
